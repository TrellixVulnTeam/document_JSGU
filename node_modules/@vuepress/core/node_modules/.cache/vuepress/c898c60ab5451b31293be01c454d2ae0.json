olute;\r\n        top: 50%;\r\n        left: 0;\r\n        right: 0;\r\n        p {\r\n          padding: 0;\r\n          margin: 0;\r\n          font-size: 48px;\r\n          font-weight: bold;\r\n          color: #FFF;\r\n        }\r\n      }\r\n    }\r\n  }\r\n</style>\r\n<template>\r\n  <div>\r\n    <div class=\"banner\">\r\n      <div class=\"banner-desc\">\r\n        <h1>网站快速成型工具</h1>\r\n        <p>Element，一套为开发者、设计师和产品经理准备的基于 Vue 2.0 的桌面端组件库</p>\r\n      </div>\r\n    </div>\r\n    <div class=\"jumbotron\" ref=\"indexMainImg\">\r\n      <img :src=\"$withBase('/index/theme-index-blue.png')\" alt=\"\">\r\n      <div class=\"jumbotron-red\" :style=\"{\r\n           height: mainImgOffset + 'px'\r\n         }\">\r\n        <img :src=\"$withBase('/index/theme-index-red.png')\" alt=\"\">\r\n      </div>\r\n    </div>\r\n    <div class=\"cards\">\r\n      <ul class=\"container\">\r\n        <li v-for=\"(item, index) in cards\" :key=\"index\">\r\n          <div class=\"card\">\r\n            <img :src=\"$withBase(`/index/${item.img}`)\" alt=\"\">\r\n            <h3>{{ item.title }}</h3>\r\n            <p>{{ item.desc }}</p>\r\n            <!-- <router-link\r\n              active-class=\"active\"\r\n              to=\"/<%= lang >/guide/design\"\r\n              exact><%= 5 >\r\n            </router-link> -->\r\n          </div>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\n  import throttle from 'throttle-debounce/throttle';\r\n  \r\n  export default {\r\n    name: 'HomeIndex',\r\n    created() {\r\n      this.throttledHandleScroll = throttle(10, true, index => {\r\n        this.handleScroll(index);\r\n      });\r\n    },\r\n    methods: {\r\n      handleScroll(index) {\r\n        const ele = this.$refs.indexMainImg;\r\n        const rect = ele.getBoundingClientRect();\r\n        const eleHeight = ele.clientHeight + 55;\r\n        let calHeight = (180 - rect.top) * 2;\r\n        if (calHeight < 0) calHeight = 0;\r\n        if 